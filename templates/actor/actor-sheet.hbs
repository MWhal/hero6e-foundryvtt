{{!--log 'HEROSYS SHEET' this--}}
<form class="hero {{cssClass}} flexcol actor-sheet" autocomplete="off">
    <nav class="sheet-edit-tabs sheet-tabs tabs" data-group="mode">
        <a class="item edit-settings" data-tab="play" data-edit="disabled">Play</a>
        <a class="item edit-settings" data-tab="edit" data-edit="enabled">Edit</a>

        {{#if isGM}}
        <select name="type" data-dtype="text">
            {{selectOptions actorTypeChoices selected=actor.type localize=true}}
        </select>
        {{else}}
        <select name="type" data-dtype="text" disabled title="Only a Game Master can change the Actor type.">
            {{selectOptions actorTypeChoices selected=actor.type localize=true }}
        </select>
        {{/if}}

        {{token}}

        <label class="upload-button">
            <span>Upload .HDC</span>
            <input type="file" />
        </label>
    </nav>

    <section class="sheet-mode">
        <section class="tab play" data-group="mode" data-tab="play">
            <header class="sheet-header">
                <img class="profile-img borderless" src="{{actor.img}}" title="{{actor.name}}" height="100"
                    width="100" />
                <div class="header-fields">
                    <h1 class="charname">
                        <input type="text" value="{{actor.name}}" placeholder="Name" disabled />
                    </h1>
                </div>
            </header>
        </section>

        <section class="tab edit" data-group="mode" data-tab="edit">
            <header class="sheet-header">
                <img class="profile-img" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="100"
                    width="100" />
                <div class="header-fields">
                    <h1 class="charname">
                        <input type="text" name="name" value="{{actor.name}}" placeholder="Name" />
                    </h1>
                </div>
            </header>
        </section>



        <section>
            <div class="grid grid-3col">
                {{#each characteristicSet.resource as |characteristic key|}}
                <div class="resource flex-group-center">
                    <label for="characteristics.{{characteristic.name}}"
                        class="resource-label">{{characteristic.name}}</label>
                    <div class="resource-content flexrow flex-center flex-between">
                        <input type="text" name="system.characteristics.{{characteristic.key}}.value"
                            value="{{characteristic.value}}" data-dtype="Number" />
                        <span> / </span>
                        <input type="text" name="system.characteristics.{{characteristic.key}}.max"
                            class="conditional-input" value="{{characteristic.max}}" data-dtype="Number" {{#unless
                            ../options.heroEditable}}disabled{{/unless}} />
                    </div>
                </div>
                {{/each}}
            </div>
            <div class="characteristics characteristic-group grid grid-4col flex1 marginless">
                {{#each characteristicSet.combat as |characteristic key|}}
                <div class="characteristic flexrow flex-group-center">
                    <label class="resource-label">{{characteristic.name}}</label>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.value"
                        value="{{characteristic.value}}" data-dtype="Number" />
                    <span>/</span>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.max"
                        class="conditional-input" value="{{characteristic.max}}" data-dtype="Number" {{#unless
                        ../options.heroEditable}}disabled{{/unless}} />
                </div>
                {{/each}}
            </div>
            <div class="characteristics characteristic-group grid grid-3col flex2 marginless">
                {{#each characteristicSet.rollable as |characteristic key|}}
                <div class="flexrow flex-group-center characteristic">
                    <label class="resource-label">{{characteristic.name}}</label>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.value"
                        value="{{characteristic.value}}" data-dtype="Number" />
                    <span>/</span>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.max"
                        class="conditional-input" value="{{characteristic.max}}" data-dtype="Number" {{#unless
                        ../options.heroEditable}}disabled{{/unless}} />
                    <button type="button" class="characteristic-roll rollable-characteristic" data-roll="3d6"
                        data-label="{{characteristic.key}}">{{numberFormat characteristic.roll decimals=0
                        sign=false}}-</button>
                </div>
                {{/each}}
            </div>
            <div class="characteristics characteristic-group grid grid-4col flex1 marginless">
                {{#each characteristicSet.defense as |characteristic key|}}
                <div class="characteristic flexrow flex-group-center">
                    <label class="resource-label">{{characteristic.name}}</label>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.value"
                        value="{{characteristic.value}}" data-dtype="Number" />
                    <span>/</span>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.max"
                        class="conditional-input" value="{{characteristic.max}}" data-dtype="Number" {{#unless
                        ../options.heroEditable}}disabled{{/unless}} />
                </div>
                {{/each}}
                {{#each characteristicSet.other as |characteristic key|}}
                <div class="characteristic flexrow flex-group-center">
                    <label class="resource-label">{{characteristic.name}}</label>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.value"
                        value="{{characteristic.value}}" data-dtype="Number" />
                    <span>/</span>
                    <input type="text" name="system.characteristics.{{characteristic.key}}.max"
                        class="conditional-input" value="{{characteristic.max}}" data-dtype="Number" {{#unless
                        ../options.heroEditable}}disabled{{/unless}} />
                </div>
                {{/each}}
        </section>

        <div class="characteristic flexrow flex-group-center">
            <button type="button" class="recovery-button" style="height:40px;width:90%;align-self:center;">Take
                Recovery</button>
            <button type="button" class="presence-button" style="height:40px;width:90%;align-self:center;">Presence
                Attack</button>
        </div>

        {{!-- Sheet Tab Navigation --}}
        <nav class="sheet-tabs sheet-item-tabs tabs" data-group="primary">
            {{#if hitLocTracking}}
            <a class="item" data-tab="hitLoc">Hit Locations</a>
            {{/if}}
            <a class="item" data-tab="movement">Movement</a>
            <a class="item" data-tab="maneuvers">Maneuvers</a>
            <a class="item" data-tab="powers">Powers</a>
            <a class="item" data-tab="equipment">Equipment</a>
            <a class="item" data-tab="attacks">Attacks</a>
            <a class="item" data-tab="defenses">Defenses</a>
            <a class="item" data-tab="skills">Skills</a>
            <a class="item" data-tab="other">Other</a>
            <a class="item" data-tab="description">Description</a>
        </nav>

        {{!-- Sheet Body --}}
        <section class="sheet-body">
            {{!-- Hit Locations Tab --}}
            <div class="tab tab-body powers" data-group="primary" data-tab="hitLoc">
                <div class="grid grid-2col">
                    {{#each system.characteristics.body.loc as |loc key|}}
                    <div class="flexrow">
                        <label>{{key}}:</label>
                        <input type="text" name="system.characteristics.body.loc.{{key}}" value="{{loc}}"
                            data-dtype="Number" />
                    </div>
                    {{/each}}
                </div>
            </div>

            {{!-- Movement Tab --}}
            <div class="tab tab-body powers" data-group="primary" data-tab="movement">
                <header class="tab-header flexrow">
                    <span class="item-image"></span>
                    <span class="item-header flex2">Name</span>
                    <span class="item-header flex3">Description</span>
                    <span class="item-header flex1">max</span>
                    <span class="item-header flex1">Value</span>
                    <span class="item-header flex1">Velocity</span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create movement" data-type="movement"><i
                                class="fas fa-plus"></i> Add movement</a>
                    </span>
                </header>
                <section class="movement-group">
                    <ol class="items-list">
                        {{#each movement as |item id|}}
                        <li class="item flexrow" data-item-id="{{item._id}}">
                            <div class="item-image"><img src="{{item.img}}" title="{{item.name}}" width="24"
                                    height="24" /></div>
                            <div class="item-label flex2">{{ item.name }}</div>
                            <div class="item-label flex3">{{ item.system.description }}</div>
                            <div class="item-name flex1">{{ item.system.max }}</div>
                            <div class="item-name flex1">{{ item.system.value }}</div>
                            <div class="item-name flex1">{{ item.system.velValue }}</div>
                            <div class="item-controls" data-group="mode">
                                <a class="item-control item-edit" title="Edit Item" id="{{id}}"><i
                                        class="fas fa-edit"></i></a>
                                <a class="item-control item-delete" title="Delete Item" id="{{id}}"><i
                                        class="fas fa-trash"></i></a>
                            </div>
                        </li>
                        {{/each}}
                    </ol>
                </section>
            </div>

            {{!-- Attacks Tab --}}
            <div class="tab tab-body powers" data-group="primary" data-tab="attacks">
                <table>
                    <tr>
                        <th></th>
                        <th>{{localize "ActorSheet.Name"}}</th>
                        <th>{{localize "ActorSheet.HitMod"}}</th>
                        <th>{{localize "ActorSheet.OffVDef"}}</th>
                        <th>{{localize "ActorSheet.Dmg"}}</th>
                        <th>{{localize "ActorSheet.Res"}}</th>
                        <th>{{localize "ActorSheet.End"}}</th>
                        <th>{{localize "ActorSheet.Aoe"}}</th>
                        <th>{{localize "ActorSheet.Roll"}}</th>
                        <th>
                            <span class="item-controls">
                                <a class="item-control item-create" title="Create attack" data-type="attack"><i class="fas fa-plus"></i> Add attack</a>
                            </span>     
                        </th>
                    </tr>
                    {{#each attacks as |item id|}}
                        <tr data-item-id="{{item._id}}">
                            <td><div class="item-image"><img src="{{item.img}}" title="{{item.name}}" width="24" height="24" /></div></td>
                            <td>{{item.name}}</td>
                            <td>{{item.toHitMod}}</td>
                            <td>{{item.uses}} vs {{item.targets}}</td>
                            <td>{{item.damage}}</td>
                            <td>{{item.class}}</td>
                            <td>{{item.end}}</td>
                            {{#if (eq item.system.areaOfEffect.type "none")}}
                                <td>-</td>
                            {{/if}}
                            {{#if (ne item.system.areaOfEffect.type "none")}}
                                <td>{{item.system.areaOfEffect.type}} ({{item.system.areaOfEffect.value}})</td>
                            {{/if}}
                            <td><a class="item-image flex1 item-attack"><i class="fas fa-dice" title="{{item.name}}" width="24" height="24" ></i></a></td>
                            <td>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                                </div>     
                            </td>
                        </tr>
                    {{/each}}
                </table>
            </div>

            {{!-- Defenses Tab --}}
            <div class="tab tab-body powers" data-group="primary" data-tab="defenses">
                <header class="tab-header flexrow">
                    <span class="item-image"></span>
                    <span class="item-name flex3">Name</span>
                    <span class="item-name flex3">Type</span>
                    <span class="item-value">Value</span>
                    <span class="item-value">Res.</span>
                    <span class="item-value">Hard.</span>
                    <span class="item-value">Imp.</span>
                    <span class="item-value">Active</span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create defense" data-type="defense"><i
                                class="fas fa-plus"></i> Add defense</a>
                    </span>
                </header>
                <section class="defenses-group">
                    <ol class="items-list">
                        {{#each defenses as |item id|}}
                        <li class="item flexrow" data-item-id="{{item._id}}">
                            <div class="item-image"><img src="{{item.img}}" title="{{item.name}}" width="24"
                                    height="24" /></div>
                            <h4 class="item-name flex3">{{item.name}}</h4>
                            <h4 class="item-name flex3">{{item.defenseType}}</h4>
                            <h4 class="item-value">{{item.value}}</h4>
                            <h4 class="item-value">{{item.resistant}}</h4>
                            <h4 class="item-value">{{item.hardened}}</h4>
                            <h4 class="item-value">{{item.impenetrable}}</h4>
                            <h4 class="item-toggle"><input type="checkbox" name="system.active" {{checked
                                    system.active}} data-dtype="Boolean" /></h4>
                            <div class="item-controls" data-group="mode">
                                <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                            </div>
                        </li>
                        {{/each}}
                    </ol>
                </section>
            </div>

            {{!-- Skills Tab --}}
            <div class="tab tab-body skills" data-group="primary" data-tab="skills">
                <header class="tab-header flexrow">
                    <span class="item-image"></span>
                    <span class="item-name">Name</span>
                    <span class="item-characterstic">Characteristic</span>
                    <span class="item-roll">Roll</span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create skill" data-type="skill"><i
                                class="fas fa-plus"></i> Add skill</a>
                    </span>
                </header>
                <section class="skills-group">
                    <ol class="items-list">
                        {{#each skills as |item id|}}
                        <li class="item" data-item-id="{{item._id}}" {{#if item.children}} style="height:auto;" {{/if}}>
                            <div class="flexrow">
                                <div class="item-image"><img src="{{item.img}}" title="{{item.name}}" width="24"
                                        height="24" /></div>
                                <h4 class="item-name">{{item.name}}</h4>
                                <h4 class="item-name">{{item.system.characteristic}}</h4>
                                <div>
                                    <button type="button" class="item-roll rollable-skill" data-roll="{{item.roll}}"
                                        data-label="{{item._id}}">{{numberFormat item.roll decimals=0
                                        sign=false}}-</button>
                                </div>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </div>
                        </li>
                        {{/each}}
                    </ol>
                </section>
            </div>

            {{!-- Maneuvers Tab --}}
            <div class="tab tab-body attacks" data-group="primary" data-tab="maneuvers">
                <header class="tab-header flexrow">
                    <div class="item-image"></div>
                    <span class="item-name flex2">Name</span>
                    <span class="item-value flex1">Phase</span>
                    <span class="item-value flex1">OCV</span>
                    <span class="item-value flex1">DCV</span>
                    <span class="item-value flex1">Active</span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create maneuver" data-type="maneuver"><i
                                class="fas fa-plus"></i> Add maneuver</a>
                    </span>
                </header>
                <section class="maneuver-group">
                    <ol class="items-list">
                        {{#each maneuvers as |item id|}}
                        <li class="item flexrow" data-item-id="{{item._id}}">
                            <div class="item-image"><img src="{{item.img}}" title="{{item.name}}" width="24"
                                    height="24" /></div>
                            <h4 class="item-name flex2">{{item.name}}</h4>
                            <h4 class="item-value flex1">{{item.system.phase}}</h4>
                            <h4 class="item-value flex1">{{item.system.ocv}}</h4>
                            <h4 class="item-value flex1">{{item.system.dcv}}</h4>
                            <h4 class="item-value item-toggle flex1"><input type="checkbox" name="system.active"
                                    {{checked system.active}} data-dtype="Boolean" /></h4>
                            <div class="item-controls" data-group="mode">
                                <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
                            </div>
                        </li>
                        {{/each}}
                    </ol>
                </section>
            </div>

            {{!-- Powers Tab --}}
            <div class="tab tab-body powers" data-group="primary" data-tab="powers">
                <header class="tab-header flexrow">
                    <span class="item-image"></span>
                    <span class="item-name flex2">Name</span>
                    <span class="item-name flex1">End</span>
                    <span class="item-name flex4">Description</span>
                    <span class="item-name flex1">Active</span>
                    <span></span>
                    {{!--
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create power" data-type="power"><i
                                class="fas fa-plus"></i> Add Power</a>
                    </span>
                    --}}
                </header>
                <section class="powers-group">
                    <div class="power-card-container">
                        <ol class="items-list">
                            {{#each powers as |powerItem key|}}
                            <li class="item flexrow" data-item-id="{{powerItem._id}}">
                                <div class="item-image"><img src="{{powerItem.img}}" width="24" height="24" /></div>
                                <span class="item-name flex2">{{powerItem.name}}</span>
                                <span class="item-name flex1">{{powerItem.system.end}}</span>
                                <span class="item-name flex4">{{powerItem.system.description}}</span>

                                <div class="item-value item-toggle flex1">
                                    {{#if powerItem.showToggle}}<input type="checkbox" name="system.active" {{checked
                                        system.active}} data-dtype="Boolean" />
                                    {{/if}}
                                </div>

                                <div class="item-controls" data-group="mode">
                                    {{!--
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    --}}

                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            <ul class="power-card-power-item-list list">
                                {{#each powerItem.system.subItems.skill as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.maneuver as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.attack as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.defense as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.movement as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                            </ul>
                            {{/each}}
                        </ol>
                    </div>
                </section>
            </div>

            {{!-- Equipment Tab --}}
            <div class="tab tab-body equipment" data-group="primary" data-tab="equipment">
                <header class="tab-header flexrow">
                    <span class="item-name flex2">Name</span>
                    <span class="item-name flex2">End</span>
                    <span class="item-name flex2">Description</span>
                    <span></span>
                    {{!--
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create equipment" data-type="equipment"><i
                                class="fas fa-plus"></i> Add Equipment</a>
                    </span>
                    --}}
                </header>
                <section class="powers-group">
                    <div class="power-card-container">
                        <ol class="items-list">
                            {{#each equipment as |powerItem key|}}
                            <li class="item flexrow" data-item-id="{{powerItem._id}}">
                                <div class="item-image"><img src="{{powerItem.img}}" width="24" height="24" /></div>
                                <span>{{powerItem.name}}</span>
                                <span>{{powerItem.system.end}}</span>
                                <span>{{powerItem.system.rules}}</span>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    {{!--
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                    --}}
                                </div>
                            </li>
                            <ul class="power-card-power-item-list list">
                                {{#each powerItem.system.subItems.skill as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.maneuver as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.attack as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                                {{#each powerItem.system.subItems.defense as |item id|}}
                                {{#if item.visible}}
                                <li class="list-item"><span>{{item.name}}</span></li>
                                {{/if}}
                                {{/each}}
                            </ul>
                            {{/each}}
                        </ol>
                    </div>
                </section>
            </div>

            {{!-- Biography Tab --}}
            <div class="tab biography" data-group="primary" data-tab="description">
                {{editor system.biography target="system.biography" rollData=rollData button=true owner=owner
                editable=editable}}
            </div>

            {{!-- Others Tab --}}
            <div class="tab tab-body other" data-group="primary" data-tab="other">
                <header class="iteam-header flexrow">
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create perk" data-type="perk"><i
                                class="fas fa-plus"></i> Perk</a>
                    </span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create talent" data-type="talent"><i
                                class="fas fa-plus"></i> Talent</a>
                    </span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create complication" data-type="complication"><i
                                class="fas fa-plus"></i> Complication</a>
                    </span>
                    <span class="item-controls">
                        <a class="item-control item-create" title="Create martialart" data-type="martialart"><i
                                class="fas fa-plus"></i> Martial Art</a>
                    </span>

                </header>
                <header class="tab-header flexrow">
                    <span class="item-name flex2">Name</span>
                    <span class="item-name flex2">Description</span>
                    <span></span>
                </header>
                <section class="powers-group">
                    <div class="power-card-container">
                        <ol class="items-list">
                            <li class="item flexrow"><span class="item-name">Perks</span></li>
                            {{#each perk as |item key|}}
                            <li class="item flexrow" data-item-id="{{item._id}}">
                                <div class="item-image"><img src="{{item.img}}" width="24" height="24" /></div>
                                <span>{{item.name}}</span>
                                <span>{{item.system.rules}}</span>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            {{/each}}
                            <li class="item flexrow"><span class="item-name">Talents</span></li>
                            {{#each talent as |item key|}}
                            <li class="item flexrow" data-item-id="{{item._id}}">
                                <div class="item-image"><img src="{{item.img}}" width="24" height="24" /></div>
                                <span>{{item.name}}</span>
                                <span>{{item.system.rules}}</span>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            {{/each}}
                            <li class="item flexrow"><span class="item-name">Complications</span></li>
                            {{#each complication as |item key|}}
                            <li class="item flexrow" data-item-id="{{item._id}}">
                                <div class="item-image"><img src="{{item.img}}" width="24" height="24" /></div>
                                <span>{{item.name}}</span>
                                <span>{{item.system.rules}}</span>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            {{/each}}
                            <li class="item flexrow"><span class="item-name">Martial Arts</span></li>
                            {{#each martialart as |item key|}}
                            <li class="item flexrow" data-item-id="{{item._id}}">
                                <div class="item-image"><img src="{{item.img}}" width="24" height="24" /></div>
                                <span>{{item.name}}</span>
                                <span>{{item.system.rules}}</span>
                                <div class="item-controls" data-group="mode">
                                    <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
                                    <a class="item-control item-delete" title="Delete Item"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            {{/each}}

                            <li class="item flexrow"><span class="item-name">ActiveEffects</span></li>
                            {{#each actor.effects as |item key|}}
                            <li class="item flexrow" data-actor-id="{{../actor.id}}" data-actor-name="{{../actor.name}}"
                                data-event-id="{{item.id}}">
                                <div class="item-image"><img src="{{item.icon}}" width="24" height="24" /></div>
                                <span>{{item.label}}</span>
                                <div class="item-controls" data-group="mode">

                                    <a class="item-control effect-delete" title="Delete ActiveEffect"><i
                                            class="fas fa-trash"></i></a>
                                </div>
                            </li>
                            {{/each}}
                        </ol>
                    </div>
                </section>
            </div>
        </section>
    </section>
</form>